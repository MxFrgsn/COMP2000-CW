        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@drawable/profile_icon_person" />

    <ImageButton
        android:id="@+id/imageButton29"
        android:layout_width="100dp"
        android:layout_height="80dp"
        android:layout_marginStart="101dp"
        android:layout_marginTop="652dp"
        android:layout_marginEnd="210dp"
        android:backgroundTint="#F3EDF7"
        android:contentDescription="Settings"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/ic_menu_manage" />

    <ImageButton
        android:id="@+id/imageButton30"
        android:layout_width="99dp"
        android:layout_height="79dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="652dp"
        android:layout_marginEnd="310dp"
        android:backgroundTint="#979797"
        android:contentDescription="Home"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/home_icon" />

    <ImageButton
        android:id="@+id/imageButton93"
        android:layout_width="51dp"
        android:layout_height="52dp"
        android:layout_marginStart="359dp"
        android:layout_marginBottom="680dp"
        android:backgroundTint="#D3CCCC"
