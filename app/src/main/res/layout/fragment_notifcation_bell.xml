<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#D3CCCC">

    <ImageButton
        android:id="@+id/imageButton78"
        android:layout_width="51dp"
        android:layout_height="52dp"
        android:layout_marginStart="359dp"
        android:layout_marginBottom="680dp"
        android:backgroundTint="#D3CCCC"
        android:contentDescription="Notification Bell"
        android:rotation="26"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/ic_popup_reminder" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="19dp"
        android:layout_height="22dp"
        android:layout_marginStart="359dp"
        android:layout_marginEnd="33dp"
        android:layout_marginBottom="709dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/ic_notification_overlay" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="17dp"
        android:layout_height="18dp"
        android:layout_marginStart="359dp"
        android:layout_marginBottom="713dp"
        android:text=" 4"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>